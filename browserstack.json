{
    "browsers": [
        {
            "browser": "chrome",
            "os": "Windows 10",
            "versions": ["latest"]
        },
        {
            "browser": "firefox",
            "os": "OS X Mojave",
            "versions": ["latest"]
          },
          {
            "browser": "edge",
            "os": "Windows 10",
            "versions": ["latest"]
          }
    ],
    
    "run_settings": {
        "cypress_config_file": "./cypress.json",
        "npm_dependencies": {
            "cypress-cucumber-preprocessor": "^4.3.1",
            "browserstack-cypress-cli": "^1.11.2",
            "@shelex/cypress-allure-plugin": "^2.23.0",
            "cypress": "^9.1.1"
          },
        "cypress_version": "9",
        "project_name": "muon-regression",
        "build_name": "muon-cypress",
        "package_config_options": {
            "cypress-cucumber-preprocessor": {
              "nonGlobalStepDefinitions": true
            }
          },
          "parallels": "4",
          "exclude": ["packages/*","allure-results/*"]
    },
    "connection_settings": {
        "local": true,
        "local_mode": "always-on",
        "local_identifier": "CypressLocalConnection1"
    },
    "disable_usage_reporting": false
}